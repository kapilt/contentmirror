<configure xmlns="http://namespaces.zope.org/zope">

  <!-- Zope 2.9 Support -->
  <subscriber
     for=".interfaces.IMirrored
          zope.app.events.interfaces.IObjectModifiedEvent"
     handler=".subscriber.objectModified" 
     />
     
  <subscriber
     for=".interfaces.IMirrored
          zope.app.events.interfaces.IObjectCopiedEvent"
     handler=".subscriber.objectCloned" 
     />     
     
</configure>     